{
    "name": "mutationevnt/multipolls",
    "description": "A Laravel package to create multi questions polls",
    "keywords": ["polls", "votes", "laravel", "php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "achraf",
            "email": "achraf.b@mutationevent.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
          "Inani\\Larapoll\\": "src/"
        }
      },
    "require": {
        "illuminate/support": "~5|~6|~7|~8"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mutationevent\\Multipolls\\MultipollsServiceProvider"
            ],
            "aliases": {
                "MultiPoll": "Mutationevent\\Multipolls\\MultipollsFacade"
            }
        }
    }
}
